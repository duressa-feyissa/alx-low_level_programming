#include <stdlib.h>
#include <string.h>
#include <stdio.h>
#include "lists.h"
/**
 * listint_len -a function that returns the number of elements in a linked list
 * @h: Point to address h hold
 * Return: the number of nodes
 */

size_t listint_len(const listint_t *h);
{
	unsigned int i;

	i = 0;
	while (h != NULL)
	{
		h = h->next;
		i++;
	}
	return (i);
}
